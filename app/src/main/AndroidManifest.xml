<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.myapplication">

    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".Activity.been.MyApplication" >
        >
        <activity android:name=".Activity.ui.SplashActivity"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.ui.MainActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Activity.ui.MeasureActivity"
                  android:screenOrientation="portrait">
        </activity>

        <activity android:name=".Activity.ui.RemindActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Activity.ui.RemindMreasureActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Activity.ui.RemindMreasureCreadActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Activity.ui.RemindEatActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Activity.ui.RemindEatCreatActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Activity.ui.CurveActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Activity.ui.GuideActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Activity.ui.IiActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Activity.ui.SetActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Activity.ui.ShowActivity"
                  android:screenOrientation="portrait">
        </activity>
        <activity android:name=".Activity.ui.PowerActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".Activity.ui.AlarmActivity"
                  android:screenOrientation="portrait"/>

        <receiver android:name=".Activity.Receiver.EatAlarmReceiver"
            >
            <intent-filter>
                <action android:name="com.eatt.alarm.clock"/>
            </intent-filter>
        </receiver>
        <receiver android:name=".Activity.Receiver.MeasureAlarmReceiver">
            <intent-filter>
                <action android:name="com.mearsure.alarm.clock"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>